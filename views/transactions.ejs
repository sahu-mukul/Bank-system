<%- include('./includes/head.ejs')%>
  <body>
    <div class="container">
       
            <table class="table">
                <thead>
                  <tr>
                    <th scope="col">Id</th>
                    <th scope="col">Account Holder</th>
                    <th scope="col">Email-Id</th>
                    <th scope="col">Account_number</th>
                    <th scope="col">Total_Balance</th>
                    
                  </tr>
                </thead>
                <tbody>
                  <% if(data.length){ 
                                
                    for(var i = 0;i < data.length;i++) { %>                 
                   <tr>
                       <td><%=(i+1)%></td>
                       <td><%=data[i].first_name%></td>
                       <td><%=data[i].email_id%></td>
                       <td><%=data[i].account_number%></td>
                       <td><%=data[i].amount%></td>
                   </tr>
               <% }
               
                }else{ %>
                    <tr>
                       <td colspan="3">No user</td>
                    </tr>
               <% } %>
                </tbody
                  
           
            </table>
        
    </div>
    
    <%- include('./includes/end.ejs') %>  





